<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<fx:root type="Tab" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<BorderPane>
			<center>
				<TextArea fx:id="textArea" editable="false" prefHeight="200.0"
					prefWidth="200.0" BorderPane.alignment="CENTER">
					<font>
						<Font name="Monospaced Regular" size="12.0" />
					</font>
				</TextArea>
			</center>
			<bottom>
				<HBox spacing="4.0" BorderPane.alignment="CENTER">
					<children>
						<Button fx:id="disconnectButton" cancelButton="true"
							mnemonicParsing="false" text="Disconnect" />
						<Pane HBox.hgrow="ALWAYS" />
						<Button fx:id="clearButton" mnemonicParsing="false" text="Clear" />
						<Button fx:id="saveButton" mnemonicParsing="false" text="Save..." />
					</children>
					<padding>
						<Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
					</padding>
				</HBox>
			</bottom>
		</BorderPane>
	</content>
</fx:root>
